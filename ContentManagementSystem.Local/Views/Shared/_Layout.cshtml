<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/CMS/cms.css" rel="stylesheet" />
</head>
<body>
    <div class="navbar navbar-fixed-top navbar-md">
        <div class="container">
            <div class="row">
                <div class="col-md-10">
                    @Html.ActionLink("TECHNICAL INFORMATION", "Index", "Question", new { area = "" }, new { @class = "navbar-brand" })
                </div>
                <div class="col-md-2">
                    @Html.Partial("_LoginPartial")
                </div>

            </div>

        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; Copyright @DateTime.Now.Year. All Rights Reserved</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
 
    @RenderSection("scripts", required: false)
</body>
</html>
